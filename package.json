{
  "name": "simongame",
  "version": "1.0.0",
  "description": "A rendition of the classic Simon Game created in ReactJS",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config ./webpack.config.js --progress",
    "dev": "webpack-dev-server --progress --inline --host 0.0.0.0 --port 5040 --colors --watch",
    "start": "node index.js",
    "start-prod": "NODE_ENV=production node index.js",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "postinstall": "rimraf dist && webpack -p --config ./webpack.production.config.js --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdlawrence/SimonGame.git"
  },
  "keywords": [
    "react",
    "react-router",
    "game"
  ],
  "author": "Jamil D Lawrence",
  "email": "jamil.lawrence@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jdlawrence/SimonGame/issues"
  },
  "homepage": "https://github.com/jdlawrence/SimonGame#readme",
  "dependencies": {
    "async": "^2.1.2",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "body-parser": "^1.17.2",
    "css-loader": "^0.28.4",
    "eslint": "^3.16.0",
    "express": "^4.15.3",
    "html-webpack-plugin": "^2.29.0",
    "path": "^0.12.7",
    "react": "^15.3.1",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^5.0.1",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.8",
    "react-sizeme": "^2.3.4",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "webpack": "^1.13.2"
  },
  "devDependencies": {
    "babel-jest": "^19.0.0",
    "enzyme": "^2.8.2",
    "jest": "^19.0.2",
    "jest-serializer-enzyme": "^1.0.0",
    "react-addons-test-utils": "^15.5.1",
    "webpack-dev-server": "^1.15.1"
  },
  "jest": {
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-enzyme"
    ]
  }
}
